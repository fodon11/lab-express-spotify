<h1>Tracks</h1>
<form action="/">
    <button type="submit">New Search</button>
</form>
<form>
    <button type="button" value="Go back!" onclick="history.back()">Back to albums</button>
</form>
{{#each items}}
    <div class="track">
        {{name}}
        <audio controls>
            <source src="{{preview_url}}" type="audio/mp3">
        </audio>
    </div>
{{/each}}